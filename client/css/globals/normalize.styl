

// :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: >>>>> BASICS


::-moz-selection{background:$white;color:$black;text-shadow:none;}
::selection{background:$white;color:$black;text-shadow:none;}
a{color:#4559ac;text-decoration:none;}
a:hover{text-decoration:underline;}
a:focus{outline:thin dotted;}
a:hover,a:active{outline:0;}
b,strong{font-weight:bold;}
em,i{font-style:italic;}
sub,sup{font-size:75%;line-height:0;position:relative;vertical-align:baseline;}
sup{top:-0.5em;}
sub{bottom:-0.25em;}
pre,code,kbd,samp{font:1em/1 monospace,sans-serif;margin:0 0 1em;padding:1em;}
pre{white-space:pre;white-space:pre-wrap;word-wrap:break-word;}
ins{background-color:#ff9;color:#000;text-decoration:none;}
mark{background-color:#ff0;color:#000;font-style:italic;font-weight:bold;}
del{text-decoration:line-through;}
abbr[title]{border-bottom:1px dotted;}
dfn{font-style:italic;}
hr{display:block;height:1px;border:0;border-top:1px solid #ccc;margin:1em 0;padding:0;}
small{font-size:85%;}


// :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: >>>>> HTML5


article,aside,details,figcaption,figure,footer,header,hgroup,nav,section{display:block;}
audio,canvas,video{display:inline-block;*display:inline;*zoom:1;}
audio:not([controls]){display:none;}
[hidden]{display:none;}
svg:not(:root){overflow:hidden;}
figure{margin:0;}


// :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: >>>>> COMMON BLOCK TAGS


p{margin:0 0 1em;}
h1{margin:0 0 24px;font:400 42px/1 Lato,Arial,sans-serif;color:#4559ac;}
h2{margin:0;font:32px/42px Lato,Arial,sans-serif;}
h3{margin:42px 0 10px;font:700 24px/32px Lato,Arial,sans-serif;text-transform:uppercase;letter-spacing:1.5px;}
h4{margin:24px 0 3px;font:24px/42px Lato,Arial,sans-serif;color:#999;}
h5{margin:24px 0 3px;font:700 18px/18px Lato,Arial,sans-serif;color:#4559ac;}
h6{margin:24px 0 3px;font:900 italic 15px/1 Lato,Arial,sans-serif;}
.image {
	margin: 0 0 22px;
	padding: 8px;
	background-color: white;
	border: 1px solid #d3d3d3;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	-moz-background-clip: padding;
	-webkit-background-clip: padding-box;
	background-clip: padding-box;
	-webkit-box-shadow: 2px 2px 14px -10px black;
	box-shadow: 2px 2px 14px -10px black;
}
.image.left{margin:5px 24px 22px -170px;}
.image.right{margin:5px -170px 22px 24px;}
img{border:0;-ms-interpolation-mode:bicubic;vertical-align:middle;line-height:0;-webkit-border-radius:3px;border-radius:3px;}
ul{margin:1em 18px;padding:0;}
ol{margin:1em 18px;padding:0;list-style-type:decimal;}
ul ul{margin:6px 18px;}
ul ol{margin:6px 18px;}
ol ol{margin:6px 18px;}
ol ul{margin:6px 18px;}
nav{margin:0 0 1em;}
nav ul{list-style:none;margin:0;}
nav ol{list-style:none;margin:0;}
dl{margin:0 0 1em;}
dt{margin:1em 0 0;}
dt:first-child{margin:0;}
dt + dt{margin:0;}
dd{margin:0 0 0 3em;}
blockquote,q{quotes:none;margin:1em 40px;font:italic 18px/24px Lato,Arial,sans-serif;color:#b3b3b3;}
q:before,q:after{content:'';content:none;}
table
	margin: 0 0 18px;
	width: 100%;
	border-collapse: collapse;
	border-spacing: 0;
	border: 0 none;
	cursor: default;
caption
	margin: 0 0 8px;
	padding: 0;
	font: bold 14px/18px Lato, Arial, sans-serif;
	letter-spacing: 1.5px;
	text-transform: uppercase;
	text-align:left;
	color: #67a4d8;
th
	padding: 8px 12px;
	vertical-align:bottom;
	background-color: #f9d573;
	color: white;
	font: 15px/21px Lato, Arial, sans-serif;
	letter-spacing: 1.5px;
	text-shadow: 0 -1px 3px #ccc;
	text-transform: uppercase;
	text-align: left;
	-webkit-transition: background-color ease .5s;
	-moz-transition: background-color ease .5s;
	transition: background-color ease .5s;
td
	padding: 6px 12px;
	vertical-align: top;
	font: 16px/20px Lato,Arial,sans-serif;
	border-bottom: 1px solid #929efa;
	-webkit-transition: background-color ease .5s;
	-moz-transition: background-color ease .5s;
	transition: background-color ease .5s;
.console h1 {margin:15px 0 0;font:28px/36px Lato,Arial,sans-serif;color:#3a3a3a;}
.console hgroup h1 {font-weight:700;}
.console hgroup h2 {font:28px/36px Lato,Arial,sans-serif;}
.console header .button {float:right;margin:3px 0 0;width:200px;}


// :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: >>>>> HELPER CLASSES AND ADDITIONAL TYPICAL STYLES


.nocallout{-webkit-touch-callout:none;}
.ellipsis{text-overflow:ellipsis;overflow:hidden;white-space:nowrap;}
.gifhidden{position:absolute;left:-100%;}
.ir{display:block;border:0;text-indent:-999em;overflow:hidden;background-color:transparent;background-repeat:no-repeat;text-align:left;direction:ltr;}
.ir br{display:none;}
.hidden{display:none !important;visibility:hidden;}
.visuallyhidden{border:0;clip:rect(0 0 0 0);height:1px;margin:-1px;overflow:hidden;padding:0;position:absolute;width:1px;}
.visuallyhidden.focusable:active,.visuallyhidden.focusable:focus{clip:auto;height:auto;margin:0;overflow:visible;position:static;width:auto;}
.invisible{visibility:hidden;}
.clearfix:before,.clearfix:after{content:"";display:table;}
.clearfix:after{clear:both;}
.clearfix{zoom:1;}
.left{float:left;}
.mid{float:left;}
.right{float:right;}
.error{color:red;}